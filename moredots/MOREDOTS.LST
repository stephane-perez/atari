> PROCEDURE variables
  max_col&=15
  buffer$=STRING$(1200,0)
  buffer%=V:buffer$
  '
  ' dot control
  '
  dot_buffer&=996
  dot_1&=dot_buffer&-4
  dot_2&=dot_buffer&-2
  '
  pn|=2
  pp|=8
  param_ud|=3
  param_u%=0
  go_back|=1                            ! go back cycling to color 1?
  complexity|=0                         ! draw simple/complex curves
  current_palette%=V:palette_mono$
RETURN
> PROCEDURE init_palettes
  palette_mono$=STRING$(64,0)
  ad%=V:palette_mono$
  FOR f=0 TO 15
    t=&H106
    DPOKE ad%+f*2,t
    DPOKE ad%+32+f*2,t
  NEXT f
  '
  my_palette1$=STRING$(64,0)
  ad%=V:my_palette1$
  FOR f=0 TO 15
    IF f>7
      t=(f-7)*&H101+INT((f-4)*0.7)*&H10
    ELSE
      t=&H1*f+(7-f)*&H100
    ENDIF
    DPOKE ad%+f*2,t
    DPOKE ad%+32+f*2,t
  NEXT f
  '
  my_palette2$=STRING$(64,0)
  ad%=V:my_palette2$
  FOR f=0 TO 15
    IF f>7
      t=(f-7)*&H101+INT((f-4)*0.7)*&H101
    ELSE
      t=&H1*f+(7-f)*&H101
    ENDIF
    DPOKE ad%+f*2,t
    DPOKE ad%+32+f*2,t
  NEXT f
  '
  ' color 0
  CARD{&HFFFF8240}=&H102
  ' color 15
  CARD{&HFFFF825E}=&H204
RETURN
> PROCEDURE light(a%,b%,c|)               ! draw dots
  ' draw simple or more complex curves
  IF complexity|=1
    ADD param_u%,param_ud|
    IF param_u%>2000
      param_u%=0
    ENDIF
  ENDIF
  '
  ' color cycling according to palette
  IF go_back|=1
    IF c|=15
      c|=1
    ENDIF
  ENDIF
  '
  PSET a%,b%,c|
  PSET 319-a%,b%,c|
  PSET a%,199-b%,c|
  PSET 319-a%,199-b%,c|
RETURN
